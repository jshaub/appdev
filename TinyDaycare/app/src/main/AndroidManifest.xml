<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.jls6595.tinydaycare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".home_screen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".collection_screen"
            android:label="@string/title_activity_collection_screen">
            <intent-filter>
                <action android:name="android.intent.action.COLLECTION" />
            </intent-filter>
        </activity>
        <activity
            android:name=".settings_screen"
            android:label="@string/title_activity_settings_screen">
            <intent-filter>
                <action android:name="android.intent.action.SETTINGS" />
            </intent-filter>
        </activity>
        <activity
            android:name=".adopt_screen"
            android:label="@string/title_activity_adopt_screen" />

    </application>

</manifest>